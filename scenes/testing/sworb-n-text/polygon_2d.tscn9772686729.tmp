script/source = "@tool
extends Polygon2D

var sword_gradient_res : GradientTexture1D = preload(\"res://scenes/testing/sworb-n-text/sword_gradient.tres\")
var original_colors : PackedColorArray
var gradient_colors : PackedColorArray
var tw : Tween

# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	# Set original colors, since \"tool\" is gonna fuzz 'em uppp
	original_colors.push_back(Color(0.0, 0.0, 0.0))
	original_colors.push_back(Color(0.016, 1.0, 1.0))
	original_colors.push_back(Color(0.663, 0.988, 0.298))
	
	texture.gradient.colors = original_colors
	gradient_colors.push_back(Color(1.0, 1.0, 1.0, 1.0))
	gradient_colors.push_back(Color(0.843, 0.322, 1.0, 1.0))
	gradient_colors.push_back(Color(1.0, 0.0, 0.298, 1.0))
	
	tw = create_tween()
	tw.bind_node(self) # do I need dis? hmm
	tw.set_ease(Tween.EASE_IN_OUT)
	tw.set_trans(Tween.TRANS_SINE)
	tw.set_loops(INF) # uhhh, yeahhhh :D
	
	tw.set_parallel(true)
	tw.tween_property(self, ^\"sword_gradient_res:gradient.colors[0]:r\",
	 sword_gradient_res.gradient.colors[0].r, 10.0)
	tw.tween_property(self, ^\"sword_gradient_res:gradient.colors[0]:g\",
	 sword_gradient_res.gradient.colors[0].g, 10.0)
	tw.tween_property(self, ^\"sword_gradient_res:gradient.colors[0]:b\" ,
	 sword_gradient_res.gradient.colors[0].b, 10.0)

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	pass
"

