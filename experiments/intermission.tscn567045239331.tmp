var rainbow_start : String = \"[rainbow freq=0.08 sat=1.0 val=0.85 speed=27.0]\"
var rainbow_end : String = \"[/rainbow]\"

var wave_start : String = \"[wave amp=50.0 freq=5.0 connected=1]\"
var wave_end : String = \"[/wave]\"

var tornado_start : String = \"[tornado radius=17.0 freq=2.0 connected=1]\"
var tornado_end : String = \"[/tornado]\"

@export var main_text : String = \"Test\"

func _ready() -> void:
	var text_start : String = rainbow_start + wave_start + tornado_start
	var text_end : String = tornado_end + wave_end + rainbow_end

	text = text_start  + \"Test\" + text_end


func _process(_delta: float) -> void:
	pass


func _on_text_edit_text_changed(extra_arg_0: String) -> void:
	print(extra_arg_0)


